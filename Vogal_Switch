
string letra;

Console.WriteLine("Digite uma letra (A a Z)");
letra = Console.ReadLine().ToLower();

switch (letra)
{
    case "a":
    case "e":
    case "i":
    case "o":
    case "u":
        Console.WriteLine("Vogal");
        break;
        default:
        Console.WriteLine("Sua letra não é uma vogal");
        break; 
        return;
}

int atletas, maior;

maior = 0;
for (int contador  = 1; contador < 12; contador++)
{
    Console.WriteLine("Digite a altura de atletas " + contador);
    atletas = Convert.ToInt32(Console.ReadLine());

    if (atletas > 190)
    {
        maior = maior + 1;
    }
}

Console.WriteLine("Os atletas que ultrapassam 190 cm s√£o " + maior);
